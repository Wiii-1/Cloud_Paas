{
    "version": 2,
    "builds": [
      {
        "src": "api/submit.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/submit",
        "dest": "api/submit.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  